 class pattern {
    public static void main(String[] args) {
        int n = 4; // number of rows
        int num = 1; // starting number
        for(int i = 1; i <= n; i++) {
            for(int j = 1; j <= i; j++) {
                System.out.print(num*num + " ");
                num++;
            }
            System.out.println();
        }
    }
}
