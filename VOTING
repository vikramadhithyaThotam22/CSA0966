import java.util.Scanner;
public class voting {
     public static void main(String[] args) {
        Scanner s= new Scanner(System.in);
        System.out.println("enter the age: ");
        int age=s.nextInt();
        if(age>=18){
            System.out.println("eligible");
        }
        else{
            System.out.println("not eligilble");
        }
        
    }
    
}
